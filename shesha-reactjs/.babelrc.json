{
  "sourceType": "unambiguous",
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": 100
        }
      }
    ],
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "apis": "./src/apis",
          "apis/*": "./src/apis/*",
          "components": "./src/components",
          "components/*": "./src/components/*",
          "hooks": "./src/hooks",
          "hooks/*": "./src/hooks/*",
          "interfaces": "./src/interfaces",
          "interfaces/*": "./src/interfaces/*",
          "providers": "./src/providers",
          "providers/*": "./src/providers/*",
          "utils": "./src/utils",
          "utils/*": "./src/utils/*"
        }
      }
    ]
  ]
}
